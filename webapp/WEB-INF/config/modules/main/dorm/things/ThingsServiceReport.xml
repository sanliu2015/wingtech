<?xml version="1.0" encoding="GBK"?>
<dbRuleDefines>
  <ReportConfigureBean id="thingsServiceReport" type="object"> 
    <property name="name">物品信息</property>
    <property name="orderBy">1</property>
	<property name="appKey">thingsService</property>
	<property name="dataSetId">thingsService</property>
    <property name="sourceDbServiceName">dbService</property>
    <property name="sourceTableRules" type="list">
      <object class="SourceTableRuleBean" id="thingsReportTable">
        <property name="sql">
        	<![CDATA[
        		 from Dorm_Things a 
        	]]>
        </property> 
		<object name="uiContainer" type="object" class="ReportUIContainer">
			<object name="datagrid" type="object" class="ReportDatagrid"  pageSize="50" canCheckbox="1" singleSelect="false" toolbar="#thingsServiceToolbar">
				<property name="customProperty" type="map">
					<entry key="striped" dataType="boolean">true</entry>
					<entry key="autoRowHeight" dataType="boolean">false</entry>
					<entry key="remoteSort" dataType="boolean">false</entry>
					<entry key="multiSort" dataType="boolean">true</entry>
					<entry key="ctrlSelect" dataType="boolean">true</entry>
					<entry key="pageList" dataType="array">50,100,300,500</entry> 
				</property>
			</object>
			<object name="toolbarContainer" type="object"  class="ToolbarContainer"  id="thingsServiceToolbar"> 
				<property name="toolbars" type="list"> 
				    <object class="ReportToolbar" className="easyui-linkbutton" name="新建" dataOptions="plain:true,iconCls:'icon-add'"  url="/main/thingsService/add.do" limitChooseRows="0"  >
						<property name="customProperty" type="map">  
							<entry key="dialogOpts">maximized:true,cache:true</entry>
				        </property>
					</object> 
					<object class="ReportToolbar" className="easyui-linkbutton" name="修改" dataOptions="plain:true,iconCls:'icon-edit'"  url="/main/thingsService/edit.do" limitChooseRows="1" >
						 <property name="customProperty" type="map"> 
							<entry key="limitChooseRows" >1</entry>
							<entry key="dialogOpts">maximized:true,cache:true</entry>
				         </property>
				         <property name="validateExp"><![CDATA[row.auditStatusId!='0']]></property>
						 <property name="validatePrompt">不符合修改约束条件！</property>
				         <property name="recParamTemplate">id=#{id}</property>
					</object> 
					<object class="ReportToolbar" className="easyui-linkbutton" name="删除" dataOptions="plain:true,iconCls:'icon-remove'"  url="/main/thingsService/json/delete.do" targetName="ajax">
					    <property name="customProperty" type="map"> 
						    <entry key="title">删除</entry>
							<entry key="limitChooseRows">2</entry> 
							<entry key="promptStatement">您确认要删除该记录吗？</entry> 
				         </property>
				        <property name="recParamTemplate">{id:#{id}}</property>
					</object>
					<object class="ReportToolbar" className="easyui-linkbutton" name="导入" dataOptions="plain:true,iconCls:'icon-excel'"  url="/main/importExcelService/init.do?appUrl=InitImportExcel&amp;planNumber=things" limitChooseRows="0"  >
						 <property name="customProperty" type="map">  
							<entry key="dialogOpts">width:800,height:400,cache:true</entry>
				         </property>
					</object> 
              	</property>   
			</object>
		</object> 
        <property name="sourceFields" type="list"> 
		    <object class="SourceFieldRuleBean" tableAliasName="a" fieldName="name" displayLable="名称" colWidth="70" dataType="string" displayColumn="1" isQueryCondition="1" />
		    <object class="SourceFieldRuleBean" tableAliasName="a" fieldName="number" displayLable="编号" colWidth="70" dataType="string" displayColumn="1" isQueryCondition="1" />
		    <object class="SourceFieldRuleBean" tableName="a" fieldName="unit" displayLable="单位" dataType="string"  dataSourceKind="1" valueIsFromDataSource="1" queryControlKind="3" isQueryCondition="1"> 
				<property name="dataSourceSql">select code,name from TS_SysCode where codeKind=110</property>
			</object>
	        <object class="SourceFieldRuleBean" tableAliasName="a" fieldName="description" displayLable="备注" dataType="string" colWidth="100" />  
			<object class="SourceFieldRuleBean" tableAliasName="a" fieldName="createDate" displayLable="创建日期" colWidth="70" dataType="date" displayColumn="1" queryControlKind="1" />
          	<object class="SourceFieldRuleBean" tableAliasName="a" fieldName="createTime" displayLable="创建时间" dataType="string" colWidth="70"/>
          	<object class="SourceFieldRuleBean" tableAliasName="a" fieldName="createdBy"  displayLable="创建人"  dataType="string" colWidth="60" /> 
	        <object class="SourceFieldRuleBean" tableAliasName="a" fieldName="id" displayLable="id" dataType="int" displayColumn="0" />  
        </property> 
      </object>
    </property>
  </ReportConfigureBean>
</dbRuleDefines>
